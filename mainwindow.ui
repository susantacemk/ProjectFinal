    border: none;
}

QMenuBar::item {
    background: transparent;
    padding: 6px 12px;
    margin: 0px 4px;
    border-radius: 4px;
}

QMenuBar::item:selected {
    background-color: #3c3f41;
    color: #ffffff;
}

QMenuBar::item:pressed {
    background-color: #505050;
}

QMenu {
    background-color: #3a3a3a;
    border: 1px solid #555555;
    color: #ffffff;
    padding: 6px;
    border-radius: 6px;
}

QMenu::item {
    background-color: transparent;
    padding: 6px 14px;
    border-radius: 4px;
    margin: 2px;
}

QMenu::item:selected {
    background-color: #5a5a5a;
}

QMenu::item:disabled {
    color: #888888;
}

QMenu::separator {
    height: 1px;
    background: #555;
    margin: 4px 0;
}
/* Toolbar Base */
QToolBar {
    /*background: qlineargradient(
        x1: 0, y1: 0, x2: 0, y2: 1,
        stop: 0 #fdfdfd,
        stop: 1 #eaeaea
    );*/
	background-color: rgb(135, 174, 199);
    border: none;               /* Adds space inside toolbar */
    spacing: 8px;               /* Space between buttons */
    height: 40px;               /* Control overall height */
	width:4px;
}

/* Tool Buttons (Modern size + look) */
QToolButton {
    background-color: transparent;
    border: 1px solid transparent;
    border-radius: 4px;
    padding: 4px 4px;          /* Controls clickable area */
    margin: 2px;
    color: #333;
    min-width: 32px;
    min-height: 32px;
    icon-size: 24px 24px;       /* Set modern icon size */
}

/* Hover Effect */
QToolButton:hover {
    background-color: #e3edf9;
    border: 1px solid #a6c0e0;
}

/* Pressed Effect */
QToolButton:pressed {
    background-color: #c5d8f2;
    border: 1px solid #7ea4d6;
}

/* Checked (Toggleable) Buttons */
QToolButton:checked {
    background-color: #d0e0f5;
    border: 1px solid #649fd1;
}

/* Toolbar Separator (subtle and clean) */
QToolBar::separator {
    background-color: #cccccc;
    width: 1px;
    margin: 6px;
}
